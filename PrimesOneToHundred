public class PrimesOneToHundred {
		public void printPrimes() {
		System.out.println("Prime numbers between 1 to 100 are:");
				for (int i = 2; i < 100; i++) {
		    	int flag=0;
			    for (int j = 2; j < i; j++) {
			    	if (i % j == 0) {
					    flag = 1;
					    break;
			      	}
		      	}

			    if (flag==0) {
			    	System.out.print(i+" ");
			    }
		  }
	}

	public static void main(String[] args) {
		PrimesOneToHundred obj = new PrimesOneHundred();
		obj.printingPrimes();
	}
}
